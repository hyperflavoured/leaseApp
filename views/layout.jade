    script(src='vendor/modernizr/modernizr.min.js')
  body
    .body
      header#header(data-plugin-options="{'stickyEnabled': true, 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': true, 'stickyStartAt': 57, 'stickySetTop': '-57px', 'stickyChangeLogo': true}")
        .header-body
          .header-container.container
            .header-row
              .header-column
                .header-logo
                  a(href='index.html')
                    img(alt='', height='100',width='160',data-sticky-width='60', data-sticky-height='80', data-sticky-top='50', src='img/logo11.png')
              .header-column
                
                  
                          
                  
                .header-row
                  .header-nav
                    //- //button.btn.header-btn-collapse-nav(data-toggle='collapse', data-target='.header-nav-main')
                    //-   i.fa.fa-bars
                    //- //ul.header-social-icons.social-icons.hidden-xs
                    //-   li.social-icons-facebook
                    //-     a(href='http://www.facebook.com/', target='_blank', title='Facebook')
                    //-      //i.fa.fa-facebook
                      
                    .header-nav-main.header-nav-main-effect-1.header-nav-main-sub-effect-1.collapse
                      nav
                        ul#mainNav.nav.nav-pills
                          
                          li
                            a.toggle(href='#/home')
                              | Home
                          //- li
                          //-   a.toggle(href='#/show_jolmohor')
                          //-     | show jolmohor
                          //- li
                          //-   a.toggle(href='#/land_registration')
                          //-     | Land Registration
                          //- //- li
                          //-   a.toggle(href='#/find_land')
                          //-     | Find Land
                          li
                            a.toggle(href='#/contact_us')
                              | Contact Us
                          //- li
                          //-   a.toggle(href='#/jolmohor_registration')
                          //-     | jolmohor


                              
                          li
                            a.toggle(href='#/about_us')
                              | About Us 
                          li(ng-if="userInfo.userType === 'admin'")
                            a.toggle(href='#/register')
                              | Register        
                          //- li
                          //-   a.toggle(href='#/user_list')
                          //-     | User List
                          li(ng-if="userInfo && userInfo.userType!='admin'")
                            a.toggle(href='#/user_land/{{userInfo.nid}}')
                              | My Lands
                          li(ng-if="userInfo")
                            a.toggle(href='#/user_update/{{userInfo._id}}')
                              | My Account
                          li(ng-if="userInfo.userType === 'admin'")
                            a.toggle(href='#/admin')
                              | Admin Panel
                          li(ng-if="loggedIn !== 'true'")
                            a.toggle(href='#/login')
                              | Log in
                          li(ng-if="loggedIn === 'true'")
                            a.btn.toggle(ng-click="logOut()") Log Out
                          li(ng-if="userInfo")
                            h2 Hello {{userInfo.username}}
                          //- li
                          //-   a.toggle(href='#/user_list')
                          //-     | Admin panel
      resovle-loader
      block content

      footer#footer
        .container
          
            .col-md-3
              .newsletter
                h4 Newsletter
                p
                  | Keep up on our always evolving product features and technology. Enter your e-mail and subscribe to our newsletter.
                #newsletterSuccess.alert.alert-success.hidden
                  strong Success!
                  |  You've been added to our email list.
                #newsletterError.alert.alert-danger.hidden
                form#newsletterForm(action='php/newsletter-subscribe.php', method='POST')
                  .input-group
                    input#newsletterEmail.form-control(placeholder='Email Address', name='newsletterEmail', type='text')
                    span.input-group-btn
                      button.btn.btn-default(type='submit') Go!
            .col-md-3
              h4 For Latest News              
              p
                | Stay conected with us.....
            .col-md-4
              .contact-details
                h4 Contact Us
                ul.contact
                  li
                    p
                      i.fa.fa-map-marker
                      strong Address:
                      |  1234 Street Name, City Name, Bangladesh
                  li
                    p
                      i.fa.fa-phone
                      strong Phone:
                      |  (123) 456-789
                  li
                    p
                      i.fa.fa-envelope
                      strong Email:
                      a(href='mailto:mail@example.com') mail@example.com
            .col-md-2
              h4 Follow Us
              ul.social-icons
                li.social-icons-facebook
                  a(href='http://www.facebook.com/', target='_blank', title='Facebook')
                    i.fa.fa-facebook
                
        .footer-copyright
          .container
            .row
              .col-md-1
                //- a.logo(href='index.html')
                //-   img.img-responsive(alt='Porto Website Template', src='img/logo-footer.png')
              .col-md-7
                p Â© Copyright 2017. All Rights Reserved.
              .col-md-4
                nav#sub-menu
                  ul
                    li
                      a(href='page-faq.html') FAQ's
                    li
                      a(href='') Terms of Use
                    
