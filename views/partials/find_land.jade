.container
	.row
		//- .col-md-2
		.col-md-6
			h1 Land List
			table#datatable-editable.table.table-bordered.table-striped.mb-none
				thead
					tr
						th Upazilla
						th  Thana
						th Postal Code
						th Jol-mohor
						th Balu-mohor
						th Chingri-mohor
						th Hat-bazar
						
					tr
						th
							input(type="text", ng-model="land.upazilla", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.area", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.postalcode", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.jolmohor", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.balumohor", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.chingrimohor", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.hatbazar", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.chingrimohor", ng-keyup="getLandList(land)")
						th
							input(type="text", ng-model="land.hatbazar", ng-keyup="getLandList(land)")
				tbody
					tr(ng-repeat = "land in landList")
						td(ng-bind="land.upazilla")
						td(ng-bind="land.area")
						td(ng-bind="land.postalcode")
						td(ng-bind="land.jolmohor")
						td(ng-bind="land.balumohor")
						td(ng-bind="land.chingrimohor")
						td(ng-bind="land.hatbazar")

	//- 					td 
